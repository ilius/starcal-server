			<method name="GET" id="getGroupList">
				<response>
					<representation mediaType="app/json"/>
					<param name="groups" type="xs:list" itemType="xs:complexType">
						<item type="xs:complexType">
							<element name="groupId" type="xs:string"/>
							<element name="title" type="xs:string"/>
