<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<application xmlns="http://localhost:9001/application.wadl">
	<resources base="http://localhost:9001/">
		<resource path="util/">
			<resource path="api-version/">
				<method name="GET" id="getApiVersion">
					<response description="just a number, '1' for API v1">
						<representation mediaType="text/plain"/>
					</response>
				</method>
			</resource>
		</resource>
		<resource path="auth/">
			<resource path="register/">
				<method name="POST" id="registerUser">
					<request>
						<param name="email" type="xs:string" required="true"/>
						<param name="password" type="xs:string" required="true"/>
						<param name="fullName" type="xs:string" required="false"/>
					</request>
					<response>
						<representation mediaType="app/json">
							<param name="token" type="xs:string" required="true"/>
						</representation>
					</response>
				</method>
			</resource>
			<resource path="login/">
				<method name="POST" id="login">
					<request>
						<param name="email" type="xs:string" required="true"/>
						<param name="password" type="xs:string" required="true"/>
					</request>
					<response>
						<representation mediaType="app/json">
							<param name="token" type="xs:string" required="true"/>
							<param name="expiration" type="xsd:dateTime" required="true"/>
						</representation>
					</response>
				</method>
			</resource>
			<resource path="logout/">
				<method name="POST" id="logout" description="invalidates/expires all valid tokens">
					<response>
						<representation mediaType="app/json"/>
					</response>
				</method>
			</resource>
			<resource path="change-password/">
				<method name="POST" id="changePassword">
					<request>
						<param name="email" type="xs:string" required="true"/>
						<param name="password" type="xs:string" required="true"/>
						<param name="newPassword" type="xs:string" required="true"/>
					</request>
					<response>
						<representation mediaType="app/json"/>
					</response>
				</method>
			</resource>
			<resource path="reset-password-request/">
				<method name="POST" id="resetPasswordRequest">
					<request>
						<param name="email" type="xs:string" required="true"/>
					</request>
					<response>
						<representation mediaType="app/json"/>
					</response>
				</method>
			</resource>
			<resource path="reset-password-action/">
				<method name="POST" id="resetPasswordAction">
					<request>
						<param name="email" type="xs:string" required="true"/>
						<param name="resetPasswordToken" type="xs:string" required="true"/>
						<param name="newPassword" type="xs:string" required="true"/>
					</request>
					<response>
						<representation mediaType="app/json"/>
					</response>
				</method>
			</resource>
		</resource>
		<resource path="me/">
			<resource path="info/">
				<method name="GET" id="getUserInfo">
					<response>
						<representation mediaType="app/json">
							<param name="email" type="xs:string"/>
							<param name="fullName" type="xs:string"/>
							<param name="defaultGroupId" type="xs:string"/>
							<param name="lastLogoutTime" type="xsd:dateTime" nillable="true" description="dateTime or none"/>
							<param name="lastLogins" type="xs:list" itemType="xs:complexType">
								<item type="xs:complexType">
									<element name="time" type="xsd:dateTime"/>
									<element name="userId" type="xs:string"/>
									<element name="email" type="xs:string"/>
									<element name="remoteIp" type="xs:string"/>
									<element name="successful" type="xs:bool" description="password was correct"/>
									<element name="locked" type="xs:bool" required="false" description="password was correct, but user was locked"/>
								</item>
							</param>
						</representation>
					</response>
				</method>
			</resource>
			<resource path="login-history/">
				<method name="GET" id="getUserInfo">
					<request>
						<param name="limit" type="xs:int" required="false" description="there is a default limit"/>
					</request>
					<response>
						<representation mediaType="app/json">
							<param name="lastLogins" type="xs:list" itemType="xs:complexType">
								<item type="xs:complexType">
									<element name="time" type="xsd:dateTime"/>
									<element name="userId" type="xs:string"/>
									<element name="email" type="xs:string"/>
									<element name="remoteIp" type="xs:string"/>
									<element name="successful" type="xs:bool" description="password was correct"/>
									<element name="locked" type="xs:bool" required="false" description="password was correct, but user was locked"/>
								</item>
							</param>
						</representation>
					</response>
				</method>
			</resource>
			<resource path="full-name/">
				<method name="PUT" id="setUserFullName">
					<request>
						<param name="fullName" type="xs:string"/>
					</request>
					<response>
						<representation mediaType="app/json"/>
					</response>
				</method>
				<method name="DELETE" id="unsetUserFullName">
					<response>
						<representation mediaType="app/json"/>
					</response>
				</method>
			</resource>
			<resource path="default-group/">
				<method name="PUT" id="setUserDefaultGroup">
					<response>
						<representation mediaType="app/json">
							<param name="defaultGroupId" type="xs:string"/>
						</representation>
					</response>
				</method>
				<method name="DELETE" id="unsetUserDefaultGroup">
					<response>
						<representation mediaType="app/json"/>
					</response>
				</method>
			</resource>
			<resource path="data-full">
				<method name="GET" id="getUserDataFull">
					<response>
						<representation mediaType="app/json">
							<param name="user" type="xs:complexType">
								<element name="email" type="xs:string"/>
								<element name="fullName" type="xs:string"/>
								<element name="defaultGroupId" type="xs:string"/>
								<element name="lastLogoutTime" type="xsd:dateTime" nillable="true" description="dateTime or none"/>
								<element name="lastLogins" type="xs:list" itemType="xs:complexType">
									<item type="xs:complexType">
										<element name="time" type="xsd:dateTime"/>
										<element name="userId" type="xs:string"/>
										<element name="email" type="xs:string"/>
										<element name="remoteIp" type="xs:string"/>
										<element name="successful" type="xs:bool" description="password was correct"/>
										<element name="locked" type="xs:bool" required="false" description="password was correct, but user was locked"/>
									</item>
								</element>
							</param>
							<param name="groups" type="xs:list" itemType="xs:complexType">
								<item type="xs:complexType">
									<element name="groupId" type="xs:string"/>
									<element name="title" type="xs:string"/>
									<element name="ownerEmail" type="xs:string"/>
								</item>
							</param>
							<param name="events" type="xs:list" itemType="xs:complexType">
								<item type="xs:complexType">
									<element name="eventId" type="xs:string"/>
									<element name="eventType" type="xs:string">
										<option value="allDayTask"/>
										<option value="custom"/>
										<option value="dailyNote"/>
										<option value="largeScale"/>
										<option value="lifeTime"/>
										<option value="monthly"/>
										<option value="task"/>
										<option value="universityClass"/>
										<option value="universityExam"/>
										<option value="weekly"/>
										<option value="yearly"/>
									</element>
									<element name="lastModifiedTime" type="xsd:dateTime"/>
									<element name="lastSha1" type="xs:string"/>
									<element name="meta" type="xs:complexType">
										<element name="ownerEmail" type="xs:string"/>
										<element name="isPublic" type="xs:boolean"/>
										<element name="creationTime" type="xsd:dateTime"/>
									</element>
									<element name="data" type="xs:complexType" description="extra elements are present depending on {eventType},
									only the common elements are documented">
										<element name="summary" type="xs:string"/>
										<element name="description" type="xs:string"/>
										<element name="summaryEncrypted" type="xs:boolean"/>
										<element name="descriptionEncrypted" type="xs:boolean"/>
										<element name="calType" type="xs:string">
											<option value="gregorian"/>
											<option value="jalali"/>
											<option value="hijri"/>
											<option value="indian_national"/>
											<option value="julian"/>
											<option value="ethiopian"/>
											<option value="gregorian_proleptic"/>
										</element>
										<element name="icon" type="xs:string"/>
										<element name="timeZone" type="xs:string"/>
										<element name="timeZoneEnable" type="xs:boolean"/>
									</element>
								</item>
							</param>
						</representation>
					</response>
				</method>
			</resource>
		</resource>
